			function logThis(e) {
			    console.log($(this).attr('id') + ': ' + e.type);
			}
			// jQuery DOM-Loaded event
			$(function() {
			    
			    $('.bullseye')

			    // Bind events
			    .bind('viewportenter', logThis)
			    .bind('viweportleave', logThis)

			    // Initialize Bullseye
			    .bullseye();

			});
</script>


